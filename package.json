{
  "name": "set-components",
  "version": "1.1.0",
  "main": "build/index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-class-properties",
      "@babel/plugin-transform-react-jsx",
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack --mode production",
    "test:unit": "mocha --require @babel/register --require ./test/helpers.js --require ./test/dom.js 'test/specs/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mikedeng/set-components.git"
  },
  "bugs": {
    "url": "https://github.com/mikedeng/set-components/issues"
  },
  "homepage": "https://github.com/mikedeng/set-components#readme",
  "author": "Mike Dune",
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "react": "^16.8.6",
    "webpack": "^4.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "react-dom": "^16.8.6",
    "webpack-cli": "^3.3.2"
  }
}
